{
  "name": "@fanly/docsjs",
  "version": "0.1.0",
  "description": "Render-first Word fidelity component with Web Component core and React/Vue adapters",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run typecheck && npm run build"
  },
  "keywords": [
    "word",
    "docx",
    "editor",
    "fidelity",
    "iframe",
    "react",
    "vue",
    "web-component"
  ],
  "homepage": "https://github.com/fanly/docsjs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanly/docsjs.git"
  },
  "bugs": {
    "url": "https://github.com/fanly/docsjs/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "vue": ">=3.4.0"
  },
  "dependencies": {
    "jszip": "^3.10.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "vue": "^3.5.13"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  }
}
